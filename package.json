{
  "name": "peach-qe",
  "version": "1.0.0",
  "description": "relay visualizer - desktop app",
  "author": "OS Labs - Team Peach - CBV",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "npm run relay && cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "relay": "relay-compiler --schema ./schema.graphql --src ./src/ --watchman false $@",
    "package": "npm run build",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Peach QE - Relay Visualizer\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "postpackage": "electron-packager ./ --out=./release-builds"
  },
  "dependencies": {
    "2": "^3.0.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.21.1",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-wildcard": "^7.0.0",
    "bootstrap": "^5.0.1",
    "codemirror": "^5.61.1",
    "get-graphql-schema": "^2.1.2",
    "moment": "^2.29.1",
    "prismjs": "^1.23.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.6.0",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.9.2",
    "react-icons": "^4.2.0",
    "react-moment": "^1.1.1",
    "react-relay": "^11.0.2",
    "react-router-dom": "^5.2.0",
    "react-search-autocomplete": "^5.2.2",
    "react-spring": "^8.0.27",
    "relay-runtime": "^11.0.2",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-relay": "^11.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "graphql": "^15.5.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "relay-compiler": "^11.0.2",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
